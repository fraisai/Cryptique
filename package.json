{
  "name": "cryptique",
  "version": "1.0.1",
  "description": "fullstack cryptocurrency dashboard",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "compile": "tsc --build",
    "create-table": "node ./server/models/sqlModel.js",
    "dev": "concurrently \"NODE_ENV=development nodemon server/server.ts\" \"NODE_ENV=development webpack serve --hot --open\"",
    "pretty-check": "prettier --check .",
    "pretty-write-back": "prettier --write ./server/controllers/ ./server/helpers ./server/models ./server/routes ./server/server.ts ./server/SFUSignalingChannel.ts",
    "pretty-write-front": "prettier --write ./client/components ./client/containers ./client/svg ./client/App.jsx ./client/index.js",
    "integration-test": "jest \"--testPathPattern='.+\\.integration\\.test\\.js$'\"",
    "lint": "eslint .",
    "serve": "cross-env NODE_ENV=development webpack-dev-server",
    "start": "cross-env NODE_ENV=production ts-node server/server.ts",
    "test": "jest --coverage",
    "unit-test": "jest \"--testPathPattern='.+\\.unit\\.test\\.js$'\""
  },
  "author": "Fariha Raisa Iftekher",
  "license": "ISC",
